command /staff <text> [<player>]:
	permission: *
	permission message: &cThis command does not exist
	trigger:
		if arg-1 is "remove":
			remove arg-2 from {staff::*}
		if arg-1 is "add":
			add arg-2 to {staff::*}
		if arg-1 is "see":
			send "[<##155966>&lTheEvent580 - Admin&r] Online staff :"
			loop {staff::*}:
				send "[<##155966>&lTheEvent580 - Admin&r] %loop-value%"

