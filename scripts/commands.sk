command /fly [<player>]:
	permission: group.spectators
	permission message: &cThis command does not exist
	trigger:
		if arg-1 is set:
			if arg-1 can fly:
				disable fly for arg-1
				send "[<##155966>&lTheEvent580 - Admin&r] Fly mode disabled for %arg-1%" to player
				if arg-1 is not player:
					send "[<##155966>&lTheEvent580 - Admin&r] Fly mode disabled by %executor%" to arg-1
			else:
				allow fly for arg-1
				send "[<##155966>&lTheEvent580 - Admin&r] Fly mode enabled for %arg-1%" to player
				if arg-1 is not player:
					send "[<##155966>&lTheEvent580 - Admin&r] Fly mode enabled by %executor%" to arg-1
		else:
			if executor is player:
				if player can fly:
					disable fly for player
					send "[<##155966>&lTheEvent580 - Admin&r] Fly mode disabled" to player
				else:
					allow fly for player
					send "[<##155966>&lTheEvent580 - Admin&r] Fly mode enabled" to player

command /mute [<player>]:
	permission: *
	permission message: &cThis command does not exist
	description: Mutes chat (or player if enter as an arg)
