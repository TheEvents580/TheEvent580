every tick:
	reloadTab()
	if {race_timer} = true:
		loop all players:
			if loop-player doesn't have permission "group.spectators":
				if {finished.%loop-player's uuid%} = false:
					add 0.05 to {race_lap_time.%loop-player's uuid%}
					add 0.05 to {race_total_time.%loop-player's uuid%}
			reloadTimer()

	if {jeu_condi} = 0:
		if {timer} or {timer_save} != 7 or 6 or 5:
			loop all players:
				if world of loop-player = "world":
					if the y-coordinate of loop-player < 4.5:
						if block at location(x-coordinate of loop-player, -6, z-coordinate of loop-player,  world of loop-player) = structure block:
							make 3 of end_rod at loop-player's location with offset vector(0.1, 0, 0.1) with extra 0.01 
							remove levitation from loop-player
							apply levitation 3 without particles whilst hiding the potion icon to loop-player for 2 tick
					if block 1 meter below loop-player = hay block:
						make loop-player execute command "spawn"
						add 1 to {compl_parcour.%loop-player's uuid%}
						updateParkour()
					if block 0 meter below loop-player = water:
						if loop-player's gamemode = adventure:
							make loop-player execute command "spawn"
					if block 0 or 1 meter below loop-player = dirt path:
						remove speed from loop-player
						apply speed 2 without particles whilst hiding the potion icon to loop-player for 1 second
					if block 1 meter below loop-player = ochre froglight:
						make loop-player execute command "spawn"
					if block 1 meter below loop-player = verdant froglight:
						teleport loop-player to location(193.5,69,-381.5,world "overworld", 0.01,0.01)
					if block 1 meter below loop-player = jigsaw block:
						if helmet of loop-player != air:
							command("pridecolor %loop-player% clear")
							send action bar "Your %name of {flag.%loop-player's uuid%}%&r has been removed" to loop-player
							clear {flag.%loop-player's uuid%}
							clear helmet of loop-player
						if {HubParkour.%loop-player's uuid%} = false:
							command("achivement %loop-player% give HubParkour")
					if block 2 meter under loop-player = powder_snow:
						if {Secret_Base_1.%loop-player's uuid%} = false:
							command("achivement %loop-player% give Secret_Base_1")

	else if {jeu_condi} = 2: #Parkour
		loop all players:
			if y-coordinate of loop-player is less than 100:
				if {parkour.main_level.%loop-player's uuid%} and {parkour.sub_level.%loop-player's uuid%} = 1:
					teleport loop-player to location(131.5, 129.00, 201.5,"parkour", -90, 0)
				else if game mode of loop-player = spectator:
					teleport loop-player to location(131.5, 129.00, 201.5,"parkour", -90, 0)
				else:
					teleport loop-player to {parkour_checkpoint_location.%loop-player's uuid%}
			if the game mode of loop-player = adventure:
				if loop-player doesn't have permission "group.spectators":
					if {finished.%loop-player's uuid%} = false:
						if {parkour.main_level.%loop-player's uuid%} = 4:
							if {parkour.sub_level.%loop-player's uuid%} = 7:
								if the inventory of loop-player contains carrot on a stick:
									clear loop-player's inventory
						if the x-coordinate of loop-player > {parkour.next_level_coord.%loop-player's uuid%}:
							if the y coordinate of loop-player > 128:
								if the z-coordinate of loop-player is between 198 and 204 :
