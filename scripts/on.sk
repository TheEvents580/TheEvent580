on place:
	if {timer} is "pause":
		if player doesn't have permission "group.spectators":
			cancel event
	else:
		if {jeu_condi} is 9:
			if block 1 meter under event-location is not quartz bricks:
				if gamemode of player is not creative:
					cancel event
					send "[&c&lTheEvent580&r] Please place the block in the replication zone only" to player

on swap hand item:
	if name of tool of player is "&eYour Bingo card":
		cancel event
	if name of tool of player is "&eTeleport to home":
		cancel event

on drop:
	if event-item is a paper named "&eYour Bingo card":
		cancel event
	if event-item is a carrot on a stick named "&eTeleport to home":
		cancel event
	if {jeu_condi} is 5 or 8 or 9:
		cancel event
	if player has permission "group.spectators":
		cancel event

on pickup:
	if player has permission "group.spectators":
		if {jeu_condi} is not 0:
			cancel event
	else:
		if {jeu_condi} is 8:
			cancel event
			make server execute command "execute at %player% run kill @e[type=minecraft:arrow,limit=1]"
		if {jeu_condi} is 9:
			set {_has_finished.%player's uuid%} to false
			loop {finished::*}:
				if loop-value is player:
					set {_has_finished.%player's uuid%} to true
			if {_has_finished.%player's uuid%} is true:
				cancel event
		if {jeu_condi} = 6 or 0:
			if {alive.%player's uuid%} = false:
				cancel event


on craft:
	if {jeu_condi} is not 3 or 6:
		cancel event

on player world change:
	loop all players:
		if loop-player has permission "thefox580.staff":
			if event-player doesn't have permission "group.spectators":
				send "[&c&lTheEvent580&r] Player &c&l%event-player%&r got teleported from &6&l%past event-world%&r to &a&l%event-world%&r, loading texture pack..." to loop-player
	send title "PLEASE STANDBY" with subtitle "for the texture pack to download" to player for 5 seconds
	wait 2 second
	if event-world is "world":
		make console execute command "setrp %player% https://download.mc-packs.net/pack/00f58e93166e5deb00f98d74f94e37f67ef84350.zip 00f58e93166e5deb00f98d74f94e37f67ef84350" #Spawn
